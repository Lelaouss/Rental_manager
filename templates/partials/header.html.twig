{# MESSAGES FLASH #}
{% for label, messages in app.flashes %}
    <div class="container">
        <div class="d-flex justify-content-end">
            <div class="col-md-3">
                <div class="alert alert-{{ label }}">
                    {% for message in messages %}
                        <p>{{ message }}</p>
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
{% endfor %}
